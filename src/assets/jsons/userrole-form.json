{
    "pageHeading": "User Role",
    "formType": "simple",
    "bind":"org_id",
    "form": {
      "collectionName": "user_role",
      "getDataEndPoint": "userrole/filter/id",
      "fields": [
        
            {
              "type": "input",
              "key": "id",
             
              "props": {
                "label": "Role Id",
                "placeholder": "Role Id",
                "required": true,
                "pattern": "^[a-zA-Z0-9-_]*$"
              }
            },
            {
              "type": "input",
              "key": "name",
              
              "props": {
                "label": "Name",
                "pattern": "^[a-zA-Z_.]+( [a-zA-Z_.]+)*$",
                "placeholder": "Name",
                "required": true
              }
            },
            
            {
                "type": "input",
                "key": "hierarchy_level",
               
                "props": {
                  "label": "Hierarchy Level",
                  "placeholder": "Hierarchy Level",
                  "required": true
                  
                 
                }
              },
              {
                "key": "role_type",
                "type": "select",
              
                "props": {
                  "type": "input",
                  "label": "Role Type",
                  "options": [
                    {
                    "label": "Active",
                    "value": "A"
                    },
                    {
                        "label": "Deactive",
                        "value": "D"
                        }
                  ],
                  "required": true
                }
              }
          
            


   
        
  
          ]
    }
  }
  